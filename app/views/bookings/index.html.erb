<h1>Bookings#index</h1>
<p>Find me in app/views/bookings/index.html.erb</p>

<% @bookings.each do |booking| %>
  <h2><%= booking.dive.location %></h2>
  <p><%= booking.quantity %> peoples</p>
  <p>Price: <%= booking.quantity * booking.dive.price %></p>

  <p><%= cl_image_tag booking.user.photo.path %></p>
  <p><%= booking.user.name %></p>
  <p><%= booking.user.bio %></p>
  <p><%= link_to "edit", edit_user_path(booking) %></p>
  <p><%= link_to "delete", user_booking_path(booking) %></p>

<% end %>
